{
    "name": "hego/support",
    "description": "Um pacote de suporte para sua aplicação Laravel 5",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Gomes",
            "email": "matheus@hego.com.br"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "artesaos/migrator": "^1.3",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "HEGO\\Support\\": "src",
            "HEGO\\Support\\Tests\\": "tests"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
